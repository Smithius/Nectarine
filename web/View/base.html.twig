        <div class="content">
            {% block content %}{% endblock %}
        </div>
    </div>
	<footer class="footer text-center" style="position: absolute;bottom: 0;width: 100%;height: 60px;background-color: #f5f5f5;">
	  <div class="container" >
		<p style ="margin: 20px 0;">Css zapewnia <a rel="bootstrap" href="http://getbootstrap.com/" target="_blank">Bootstrap</a>.</p>
	  </div>
	</footer>
	{% endblock %}
